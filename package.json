{
  "name": "electron-web-chapter",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "browseit/browseit.js",
  "scripts": {
    "start": "electron .",
    "start:react": "electron ./react/",
    "start:browseit": "electron ./browseit/browseit.js",
    "start:vgnews": "electron ./vgnews/vg-window.js",
    "build:osx": "electron-packager . --platform=darwin --out=dist/osx --ignore=dist --ignore=.idea",
    "build:linux": "electron-packager . --platform=linux --arch=x64 --out=dist/linux --ignore=dist --ignore=.idea",
    "build:win": "electron-packager . --platform=win32 --out=dist/win --ignore=dist --ignore=.idea"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^1.4.1"
  },
  "dependencies": {
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-register": "^6.18.0",
    "electron-packager": "^8.3.0",
    "react": "^15.4.0",
    "react-dom": "^15.4.0"
  }
}
